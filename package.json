{
  "name": "@wrangle/cli",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "bin": "bin/wrangle.ts",
  "engines": {
    "node": "^20.0.0",
    "pnpm": "^8.0.0"
  },
  "packageManager": "pnpm@^8.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "tsx bin/wrangle.ts analyze --files examples/hello-world.ts",
    "lint:depcheck": "depcheck .",
    "lint:biome": "biome check .",
    "lint:typescript": "tsc --noEmit",
    "lint": "biome check . --apply && tsc --noEmit",
    "test": "node --import tsx --test **/*.test.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "winston": "~3.13.0",
    "yargs": "~17.7.2",
    "zod": "~3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@tsconfig/node-lts": "~20.1.3",
    "@types/node": "~20.11.25",
    "@types/yargs": "~17.0.32",
    "depcheck": "~1.4.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsx": "~4.7.1",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "biome check --apply",
      "prettier --write"
    ]
  }
}
